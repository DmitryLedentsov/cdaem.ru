{
    "private": true,
    "scripts": {
        "gulp": "./node_modules/.bin/gulp",
        "dev": "npm run development",
        "development": "./node_modules/.bin/gulp html && cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "production": "./node_modules/.bin/gulp && cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "browser-sync": "^2.26.13",
        "browser-sync-webpack-plugin": "^2.0.1",
        "cross-env": "5.2.1",
        "filename-regex": "^2.0.1",
        "laravel-mix": "5.0.5",
        "lodash": "^4.17.20",
        "resolve-url-loader": "2.3.2",
        "sass": "1.26.10",
        "sass-loader": "8.0.2",
        "vue-template-compiler": "^2.6.12",
        "gulp": "4.0.2",
        "gulp-preprocess": "3.0.3"
    }
}
